{
    "name": "iprastha/bank2json",
    "description": "Convert bank files such as csv transaction to standardized json structure to be used in accounting or financial systems",
    "keywords": [
        "iprastha",
        "bank2json"
    ],
    "homepage": "https://github.com/iprastha/bank2json",
    "license": "MIT",
    "authors": [
        {
            "name": "Indra Prastha",
            "email": "iprastha@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^3.8"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Bank2json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bank2json\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
